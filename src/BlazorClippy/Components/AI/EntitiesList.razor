
<Row Margin="Margin.Is5.FromTop">
    <Column>
        <Row Margin="Margin.Is2.FromTop">
            <Column>
                <Heading Size="HeadingSize.Is6">Entities:</Heading>
            </Column>
        </Row>
        @foreach (var entity in Item.Entities)
        {
            var iscaptured = Item.FoundEntities.ContainsKey($"{entity.Entity}:{entity.Value}");
            <EntityItemSmall Entity="@entity" IsCaptured="@iscaptured" />
        }
    </Column>
</Row>

@code {
    [Parameter]
    public AnalyzedObjectDataItem Item { get; set; } = new AnalyzedObjectDataItem();
}
